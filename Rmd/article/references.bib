isbn = {1873-3360 (Electronic)$\backslash$r0306-4530 (Linking)},
issn = {03064530},
journal = {Psychoneuroendocrinology},
keywords = {Bipolar disorder,Cytokines,Lymphocytes,MAPK,Regulatory T cells,T cells},
