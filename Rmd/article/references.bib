keywords = {Alcoholic liver disease,Celiac disease,Collagen vascular disease,Food allergy,Inflammatory bowel disease,Intestinal barrier,Intestinal permeability,Lactulose,Mannitol,Sucralose,Sucrose},
month = {may},
number = {5},
