pages = {9--15},
pmid = {20934281},
publisher = {Elsevier Ltd},
title = {{Nonparental care and infant health: Do number of hours and number of concurrent arrangements matter?}},
url = {http://dx.doi.org/10.1016/j.earlhumdev.2010.09.003 http://linkinghub.elsevier.com/retrieve/pii/S0378378210002380},
