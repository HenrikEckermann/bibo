issn = {13501925},
journal = {Neurogastroenterology {\&} Motility},
keywords = {Brain-gut axis,Chronic psychological stress,Intestinal barrier,Probiotics,brain-gut axis,chronic psychological stress,intestinal barrier,probiotics},
month = {apr},
