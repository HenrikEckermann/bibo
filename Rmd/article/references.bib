pmid = {20876311},
title = {{Helminth secretions induce de novo T cell Foxp3 expression and regulatory function through the TGF- pathway}},
url = {http://www.jem.org/cgi/doi/10.1084/jem.20101074},
volume = {207},
year = {2010}
}
