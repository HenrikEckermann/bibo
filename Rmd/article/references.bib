issn = {1878-5883},
journal = {Journal of the neurological sciences},
keywords = {Adolescent,Adult,Age Distribution,Age Factors,Aged,Confidence Intervals,Environment,Female,Guadeloupe,Guadeloupe: epidemiology,Humans,Incidence,Longitudinal Studies,Male,Martinique,Martinique: epidemiology,Middle Aged,Multiple Sclerosis,Multiple Sclerosis: epidemiology,Multiple Sclerosis: etiology,Neuromyelitis Optica,Neuromyelitis Optica: epidemiology,Odds Ratio,Retrospective Studies,Sex Factors,Sex Ratio,Young Adult},
month = {nov},
number = {1-2},
pages = {58--61},
pmid = {19520388},
title = {{Environmental changes and epidemiology of multiple sclerosis in the French West Indies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19520388},
volume = {286},
year = {2009}
}
