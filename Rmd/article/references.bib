journal = {The Journal of Immunology},
keywords = {Animals,Antibodies,Antigens,Blocking,Blocking: pharmacology,Brugia malayi,Brugia malayi: growth {\&} development,Brugia malayi: immunology,CD,CTLA-4 Antigen,Cells,Cultured,Cytokines,Cytokines: biosynthesis,Differentiation,Differentiation: immunology,Differentiation: metabolism,Down-Regulation,Down-Regulation: immunology,Elephantiasis,Filarial,Filarial: immunology,Filarial: parasitology,Forkhead Transcription Factors,Forkhead Transcription Factors: biosynthesis,Forkhead Transcription Factors: genetics,GATA3 Transcription Factor,GATA3 Transcription Factor: antagonists {\&} inhibito,Host-Parasite Interactions,Host-Parasite Interactions: immunology,Humans,Immune Tolerance,Immune Tolerance: genetics,Signal Transduction,Signal Transduction: genetics,Signal Transduction: immunology,Suppressor of Cytokine Signaling Proteins,Suppressor of Cytokine Signaling Proteins: biosynt,Suppressor of Cytokine Signaling Proteins: genetic,T-Box Domain Proteins,Th1 Cells,Th1 Cells: immunology,Th1 Cells: metabolism,Th1 Cells: parasitology,Th2 Cells,Th2 Cells: immunology,Th2 Cells: metabolism,Th2 Cells: parasitology,Transcription Factors,Transcription Factors: antagonists {\&} inhibitors,Transforming Growth Factor beta,Transforming Growth Factor beta: biosynthesis,Transforming Growth Factor beta: genetics,Ubiquitin-Protein Ligases,Ubiquitin-Protein Ligases: antagonists {\&} inhibitor},
