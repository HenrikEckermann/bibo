@article{Najjar2012,
abstract = {INTRODUCTION: Glutamic acid decarboxylase (GAD) is the rate-limiting enzyme converting glutamate into $\gamma$-aminobutyric acid. Impaired GAD function can alter motor, cognitive, and behavioral function. Anti-GAD antibodies (GADAbs) can cause several neurological disorders. However, the association between anti-GADAbs and pure psychosis, without seizures or focal neurological deficits, is not well defined.

