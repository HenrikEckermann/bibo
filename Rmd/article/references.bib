journal = {Proceedings of the National Academy of Sciences},
month = {may},
number = {21},
pages = {8334--8339},
pmid = {22566627},
title = {{Environmental biodiversity, human microbiota, and allergy are interrelated}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1205624109},
volume = {109},
year = {2012}
}
