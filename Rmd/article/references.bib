pages = {1},
pmid = {26870888},
title = {{Nutrition, gut microbiota and child health outcomes}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00075197-900000000-99384},
volume = {19},
