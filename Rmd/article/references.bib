journal = {Clinical and Experimental Immunology},
keywords = {Allergy,Autoimmunity,Immunoregulation,Infection,Therapy},
number = {1},
pages = {38--46},
pmid = {24749722},
title = {{Helminths in the hygiene hypothesis: Sooner or later?}},
volume = {177},
year = {2014}
}
