keywords = {behavior,childcare center,cortisol,infant,maternal,negative emotionality,sensitivity},
month = {jan},
number = {1},
pages = {8--17},
pmid = {26455788},
title = {{Cortisol levels of infants in center care across the first year of life: links with quality of care and infant temperament}},
url = {http://www.tandfonline.com/doi/full/10.3109/10253890.2015.1089230},
