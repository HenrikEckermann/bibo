@article{Raison2013,
abstract = {CONTEXT: Increased concentrations of inflammatory biomarkers predict antidepressant nonresponse, and inflammatory cytokines can sabotage and circumvent the mechanisms of action of conventional antidepressants.$\backslash$n$\backslash$nOBJECTIVES: To determine whether inhibition of the inflammatory cytokine tumor necrosis factor (TNF) reduces depressive symptoms in patients with treatment-resistant depression and whether an increase in baseline plasma inflammatory biomarkers, including high-sensitivity C-reactive protein (hs-CRP), TNF, and its soluble receptors, predicts treatment response.$\backslash$n$\backslash$nDESIGN: Double-blind, placebo-controlled, randomized clinical trial.$\backslash$n$\backslash$nSETTING: Outpatient infusion center at Emory University in Atlanta, Georgia.$\backslash$n$\backslash$nPARTICIPANTS: A total of 60 medically stable outpatients with major depression who were either on a consistent antidepressant regimen (n = 37) or medication-free (n = 23) for 4 weeks or more and who were moderately resistant to treatment as determined by the Massachusetts General Hospital Staging method.$\backslash$n$\backslash$nINTERVENTIONS: Three infusions of the TNF antagonist infliximab (5 mg/kg) (n = 30) or placebo (n = 30) at baseline and weeks 2 and 6 of a 12-week trial.$\backslash$n$\backslash$nMAIN OUTCOME MEASURES: The 17-item Hamilton Scale for Depression (HAM-D) scores.$\backslash$n$\backslash$nRESULTS: No overall difference in change of HAM-D scores between treatment groups across time was found. However, there was a significant interaction between treatment, time, and log baseline hs-CRP concentration (P = .01), with change in HAM-D scores (baseline to week 12) favoring infliximab-treated patients at a baseline hs-CRP concentration greater than 5 mg/L and favoring placebo-treated patients at a baseline hs-CRP concentration of 5 mg/L or less. Exploratory analyses focusing on patients with a baseline hs-CRP concentration greater than 5 mg/L revealed a treatment response (≥50{\%} reduction in HAM-D score at any point during treatment) of 62{\%} (8 of 13 patients) in infliximab-treated patients vs 33{\%} (3 of 9 patients) in placebo-treated patients (P = .19). Baseline concentrations of TNF and its soluble receptors were significantly higher in infliximab-treated responders vs nonresponders (P {\textless} .05), and infliximab-treated responders exhibited significantly greater decreases in hs-CRP from baseline to week 12 compared with placebo-treated responders (P {\textless} .01). Dropouts and adverse events were limited and did not differ between groups.$\backslash$n$\backslash$nCONCLUSIONS: This proof-of-concept study suggests that TNF antagonism does not have generalized efficacy in treatment-resistant depression but may improve depressive symptoms in patients with high baseline inflammatory biomarkers.$\backslash$n$\backslash$nTRIAL REGISTRATION: clinicaltrials.gov Identifier: NCT00463580.},
author = {Raison, Charles L and Rutherford, Robin E and Woolwine, Bobbi J and Shuo, Chen and Schettler, Pamela and Drake, Daniel F and Haroon, Ebrahim and Miller, Andrew H},
doi = {10.1001/2013.jamapsychiatry.4},
file = {:Users/henrikeckermann/Library/Application Support/Mendeley Desktop/Downloaded/Raison et al. - 2013 - A Randomized Controlled Trial of the Tumor Necrosis Factor Antagonist Infliximab for Treatment-Resistant Depressi.pdf:pdf},
