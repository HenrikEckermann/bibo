issn = {1550-9109},
journal = {Sleep},
keywords = {circadian misalignment.,delayed sleep,depression},
month = {jan},
number = {1},
pages = {1--35},
pmid = {28364473},
title = {{Prevalence of Circadian Misalignment and Its Association With Depressive Symptoms in Delayed Sleep Phase Disorder.}},
url = {https://academic.oup.com/sleep/article/doi/10.1093/sleep/zsw002/2957267/Prevalence-of-Circadian-Misalignment-and-Its http://www.ncbi.nlm.nih.gov/pubmed/28364473},
